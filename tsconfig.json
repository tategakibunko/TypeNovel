{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es2019",
    ],
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "src/**/*.test.ts",
    "node_modules",
    "dist",
    "ts-example",
  ]
}
